{
    "env": {
        "browser": true,
        "es2021": true
    },
    "plugins": ["prettier"],
    "extends": ["eslint:recommended", "prettier"],
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module"
    },
    "rules": {
        "no-var":"error",
        "prettier/prettier": "error"
    }
}
